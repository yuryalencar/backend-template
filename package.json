{
  "name": "backend-template",
  "version": "0.0.1",
  "description": "Example of backend project",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "nodemon",
    "start:dev:full": "pnpm db:migrate && nodemon app/index.js",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed --seed",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:restart": "pnpm run db:migrate && pnpm run db:seed:all",
    "start:prod": "pnpm run build && node build/index.js",
    "start:prod:server": "pm2 start build --name backend-template-project"
  },
  "keywords": ["sequelize", "nodejs", "express", "vitest", "typescript"],
  "author": "Yury Alencar",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.23",
    "@vitest/coverage-v8": "^1.3.1",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
